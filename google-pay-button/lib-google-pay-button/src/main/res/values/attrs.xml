<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Reusable and styleable Google Pay button. -->
    <declare-styleable name="GooglePayButton">
        <!-- Set scale of the content inside button (logotype or text). Default is 1. -->
        <attr name="gpbContentScale" format="float" />
        <attr name="gpbStyle" format="enum">
            <enum name="black_gpay" value="0" />
            <enum name="black_buy_with_gpay" value="1" />
            <enum name="black_flat_gpay" value="2" />
            <enum name="black_flat_buy_with_gpay" value="3" />
            <enum name="white_gpay" value="4" />
            <enum name="white_buy_with_gpay" value="5" />
            <enum name="white_flat_gpay" value="6" />
            <enum name="white_flat_buy_with_gpay" value="7" />
        </attr>
    </declare-styleable>
</resources>