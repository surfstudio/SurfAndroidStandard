<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.surfstudio.android.loadstate.sample">

    <application
        android:name="ru.surfstudio.android.sample.dagger.app.DefaultApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.screen.main.MainActivityView"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.screen.stubs.RendererWithStubsDemoActivityView" />
        <activity android:name=".ui.screen.ordinary.DefaultRendererDemoActivityView" />
    </application>

</manifest>